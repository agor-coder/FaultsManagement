<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE payara-web-app PUBLIC "-//Payara.fish//DTD Payara Server 4 Servlet 3.0//EN" "https://docs.payara.fish/schemas/payara-web-app_4.dtd">
<payara-web-app error-url="">
  <context-root>/fm</context-root>
  <security-role-mapping>
    <role-name>AppAdmin</role-name>
    <group-name>Administrator</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Assigner</role-name>
    <group-name>Przydzielający</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Notifier</role-name>
    <group-name>Zgłaszający</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Specialist</role-name>
    <group-name>Specjalista</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>AUTHENTICATOR</role-name>
    <principal-name>TheMightyAuthenticator</principal-name>
  </security-role-mapping>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</payara-web-app>
